{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Codeband",
	"patterns": [
		{
			"name": "comment.line.double-slash.codeband",
			"match": "^\\s*@.*$",
			"comment": ""
		},
		{
			"name": "string.quoted.double.codeband",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.codeband",
					"match": "\\\\."
				}
			],
			"comment": "文字列"
		},
		{
			"name": "entity.name.type.codeband",
			"match": "(>>|<<)",
			"comment": "music start/end"
		},
		{
			"name": "keyword.codeband",
			"match": "(title|timesign|tempo|composer)",
			"comment": ""
		},
		{
			"name": "keyword.control.codeband",
			"match": "[a-zA-Z][a-zA-Z_0-9]*:",
			"comment": ""
		},
		{
			"name": "keyword.control.codeband",
			"match": "[a-zA-Z]+[0-9]*->",
			"comment": ""
		},
		{
			"name": "variable.codeband",
			"match": "\\|",
			"comment": ""
		},
		{
			"name": "entity.name.tag.codeband",
			"match": "(\\[|\\]|\\(|\\))",
			"comment": ""
		},
		{
			"name": "support.type",
			"match": "(chord|trip|\\{|\\})",
			"comment": ""
		},
		{
			"name": "support.type",
			"match": "\\$[a-zA-Z][a-zA-Z_0-9]*",
			"comment": ""
		},
		{
			"name": "support.type",
			"match": "(APiano|BPiano|EPiano|HPiano|EPiano1|EPiano2|Harpsichord|Clavi|Celesta|Glockenspiel|MusicBox|Vibraphone|Marimba|Xylophone|TubularBells|Dulcimer|DrawbarOrgan|PercussiveOrgan|RockOrgan|ChurchOrgan|ReedOrgan|Accordion|Harmonica|TangoAccordion|NGuitar|SGuitar|EJGuitar|ECGuitar|EMGuitar|OGuitar|DGuitar|HGuitar|ABass|EFBass|EPBass|FBass|SlBass1|SlBass2|SyBass1|SyBass2|Violin|Viola|Cello|Contrabass|TStrings|PStrings|OrchestralHarp|Timpani|SEnsemble1|SEnsemble2|SyStrings1|SyStrings2|ChoirAahs|VoiceOohs|SynthVoice|OrchestraHit|Trumpet|Trombone|Tuba|MutedTrumpet|FrenchHorn|BrassSection|SyBrass1|SyBrass2|SSax|ASax|TSax|BSax|Oboe|EnglishHorn|Bassoon|Clarinet|Piccolo|Flute|Recorder|PanFlute|BlownBottle|Shakuhachi|Whistle|Ocarina|Lead1|Lead2|Lead3|Lead4|Lead5|Lead6|Lead7|Lead8|Pad1|Pad2|Pad3|Pad4|Pad5|Pad6|Pad7|Pad8|FX1|FX2|FX3|FX4|FX5|FX6|FX7|FX8|Sitar|Banjo|Shamisen|Koto|Kalimba|Bagpipe|Fiddle|Shanai|TinkleBell|Agogo|SteelDrums|Woodblock|TaikoDrum|MelodicTom|SynthDrum|ReverseCymbal|GuitarFretNoise|BreathNoise|Seashore|BirdTweet|TelephoneRing|Helicopter|Applause|Gunshot|AcousticBassDrum|BassDrum1|SideStick|AcousticSnare|HandClap|ElectricSnare|LowFloorTom|ClosedHiHat|HighFloorTom|PedalHiHat|LowTom|OpenHiHat|LowMidTom|HiMidTom|CrashCymbal1|HighTom|RideCymbal1|ChineseCymbal|RideBell|Tambourine|SplashCymbal|Cowbell|CrashCymbal2|Vibraslap|RideCymbal2|HiBongo|LowBongo|MuteHiConga|OpenHiConga|LowConga|HighTimbale|LowTimbale|HighAgogo|LowAgogo|Cabasa|Maracas|ShortWhistle|LongWhistle|ShortGuiro|LongGuiro|Claves|HiWoodBlock|LowWoodBlock|MuteCuica|OpenCuica|MuteTriangle|OpenTriangle|Percussion)",

			"comment": ""
		}
	],
	"scopeName": "text.codeband"
}